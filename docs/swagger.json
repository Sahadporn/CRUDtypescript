{
  "swagger": "2.0",
  "info": {
    "version": "1.0.0",
    "title": "CRUD with Typescript",
    "license": {
      "name": "MIT",
      "url": "https://opensource.org/licenses/MIT"
    }
  },
  "host": "127.0.0.1:3000/profile",
  "basePath": "/",
  "schemes": ["http"],
  "consumes": ["application/json"],
  "produces": ["application/json"],
  "paths": {
    "/": {
      "get": {
        "summary": "Get all student profile",
        "produces": ["application/json"],
        "responses": {
          "200": {
            "description": "success",
            "schema": {
              "$ref": "#/definitions/AllProfile"
            }
          },
          "500": {
            "description": "Bad data"
          }
        }
      },
      "post": {
        "summary": "Create new student profile",
        "consumes": ["application/json"],
        "parameters": [
          {
            "name": "profile",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/NewProfile"
            }
          }
        ],
        "produces": ["application/json"],
        "responses": {
          "201": {
            "description": "success"
          },
          "500": {
            "description": "Bad post data"
          }
        }
      }
    },
    "/{id}": {
      "get": {
        "summary": "Get requested student data",
        "produces": ["application/json"],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "ID of student to fetch",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "success",
            "schema": {
              "$ref": "#/definitions/Profile"
            }
          },
          "500": {
            "description": "Bad data"
          }
        }
      },
      "put": {
        "summary": "Update a student profile",
        "consumes": ["application/json"],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "ID of student to update",
            "required": true,
            "type": "string"
          },
          {
            "name": "profile",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/PostProfile"
            }
          }
        ],
        "produces": ["application/json"],
        "responses": {
          "201": {
            "description": "success"
          },
          "500": {
            "description": "Bad data"
          }
        }
      },
      "delete": {
        "summary": "Delete requested student data",
        "parameters": [
            {
              "name": "id",
              "in": "path",
              "description": "ID of student to delete",
              "required": true,
              "type": "string"
            }
          ],
          "responses": {
            "201": {
              "description": "success"
            },
            "500": {
              "description": "Bad data"
            }
          }
      }
    }
  },
  "definitions": {
    "Profile": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "age": {
          "type": "integer"
        },
        "address": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "created_date": {
          "type": "string"
        },
        "updated_date": {
          "type": "string"
        }
      }
    },
    "AllProfile": {
        "type": "array",
        "items": {
            "$ref": "#/definitions/Profile"
        }
      },
    "NewProfile": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "age": {
          "type": "integer"
        },
        "address": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      }
    },
    "PostProfile": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "age": {
            "type": "integer"
          },
          "address": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        }
      }
  }
}
